@page
@model CareerFrameworkAPI.Pages.Admin.SecurityModel
@{
    ViewData["Title"] = "Privacy Policy";
}

are you an admin @Model.isAdmin <br />

compiler conditions are @Model.compilerConditions


<dl>
    @foreach (var claim in User.Claims)
    {
        <dt>@claim.Type</dt>
        <dd>@claim.Value</dd>
    }
</dl>

@if (User.IsInRole("CareerFrameworkAdmin"))
{
    <h3 class="message">Career Framework Admin</h3>
}

<form method="post" novalidate="novalidate">

    <button action="submit">LOG OUT</button>
</form>